scalar Upload

type Deck {
  _id: String!
  subject: String!
  module: String!
  moduleAlt: String
  examiners: String
  semester: String
  year: Int
  hash: String!
}

type Query {
  decks: [Deck!]!
  getDeck(id: String!): Deck
}

input NewDeck {
  subject: String!
  module: String!
  moduleAlt: String
  examiners: String
  semester: String
  year: Int
  file: Upload!
}

type Mutation {
  createDeck(input: NewDeck!): Deck!
}
